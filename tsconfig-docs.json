{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "rootDir": "sources"
    },
    "include": [
        "sources/*.ts",
        "sources/lib/*.ts"
    ],
    "typedocOptions": {
        "excludeExternals": true,
        "excludeNotExported": true,
        "excludePrivate": true,
        "excludeProtected": true,
        "includeDeclarations": true,
        "gitRevision": "master",
        "mode": "file",
        "name": "ORDBOK Core API",
        "plugin": "none",
        "theme": "node_modules/@ordbok/typedoc-theme/bin/ordbok"
    }
}
